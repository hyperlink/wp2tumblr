{
  "manifest_version": 2,
  "name": "Wordpress to Tumblr migration tool",
  "version": "0.0.1",
  "minimum_chrome_version": "23",
  "icons": {
	"16": "images/icon_16.png",
	"128": "images/icon_128.png"
  },
  "sandbox": {
    "pages": ["callback.html"]
},
  "app": {
	"background": {
	  "scripts": ["js/main.js"]
	}
  },
  "permissions": [
  	"*://*.tumblr.com/",
    "storage",
    "experimental",
    "identity"
  ],
  "key": "d3AydHVtYmxy"
}